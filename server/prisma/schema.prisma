datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime  @default(now())
  firstName DateTime?
  id        String    @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime  @updatedAt
  username  String    @unique
}

model Test {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}
